      background-color: #ffffff;
      border: 1px solid black;
      opacity: 0.6;
    }

    .transbox p {
      margin: 5%;
      font-weight: bold;
      color: #000000;
    }
    .column {
      padding-left: 10px;
      padding-right: 10px;
      column-gap: 100px;
      float: left;
      width: 20%;
    }
        .row:after {
      content: "";
      display: table;
      clear: both;
    }
        .full-footer{
      background-color: #192a56;
      width: 100%;
